<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="th_view_seeding_acceptance_tree" model="ir.ui.view">
        <field name="name">th.view.seeding.acceptance.tree</field>
        <field name="model">th.acceptance.seeding</field>
        <field name="arch" type="xml">
            <tree editable="bottom" string="Version">
                <field name="th_acceptance_type"/>
                <field name="th_coefficient"/>
                <field name="th_coefficient_convention"/>
                <field name="th_cost_factor"/>
            </tree>
        </field>
    </record>

    <record id="th_view_seeding_acceptance_action" model="ir.actions.act_window">
        <field name="name">Hoa há»“ng seeding</field>
        <field name="res_model">th.acceptance.seeding</field>
        <field name="view_id" ref="th_view_seeding_acceptance_tree"/>
    </record>
</odoo>