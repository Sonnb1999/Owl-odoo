<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="car.ListView.Buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <button if="props.model == 'th.cars'" type="button" class="btn btn-primary" aria-label="Connect" t-on-click="onClickCarDemo">
                Car demo
            </button>
        </xpath>
    </t>

</templates>
