<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="car.ListView.Buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <button t-if="props.resModel == 'th.cars'" type="button" class="btn btn-primary ml-2" t-on-click="onClickCarDemo">
                Car demo
            </button>

            <button t-if="props.resModel == 'th.automobile.company'" type="button" class="btn btn-primary ml-2" t-on-click="onClickCompanyDemo">
                Company demo
            </button>
        </xpath>
    </t>

</templates>
